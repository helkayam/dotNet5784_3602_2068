             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        
        <local:FilterWorker x:Key="FilterWorkerKey"/>
        <local:WorkerExperienceCollection x:Key="WorkerExperienceCollectionKey" />
        <local:ConvertIdToContent x:Key="ConverIdToContentKey"/>
        <local:ConvertIdToBool x:Key="ConverIdToBoolKey"/>
        <local:BoolToVisibilityConverter x:Key="BoolToVisibilityConverterKey"/>
        <local:FilterTask x:Key="FilterTaskKey"/>
        <local:StatusTaskCollection x:Key="TaskStatusKey"/>

    </Application.Resources>
</Application>
